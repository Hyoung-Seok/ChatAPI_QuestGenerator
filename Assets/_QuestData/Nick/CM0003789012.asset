  title: "\uC2A4\uCE90\uBE48\uC800\uC640\uC758 \uCCAB \uC870\uC6B0"
  questType: 0
  npcName: "\uB2C9"
  targetInfo:
  - targetName: "\uC2A4\uCE90\uBE48\uC800"
    targetCount: 1
